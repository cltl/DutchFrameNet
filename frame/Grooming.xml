    <semType name="Time" ID="141"/>
  </FE>
  <FE bgColor="008000" fgColor="FFFFFF" coreType="Peripheral" cBy="ACW" cDate="04/08/2002 12:38:51 PDT Mon" abbrev="Place" name="Place" ID="2586">
    <definition>&lt;def-root&gt;Where the event takes place.&lt;/def-root&gt;</definition>
    <semType name="Locative_relation" ID="182"/>
  </FE>
  <FE bgColor="808080" fgColor="FFFFFF" coreType="Extra-Thematic" cBy="RLG" cDate="04/15/2003 02:07:52 PDT Tue" abbrev="" name="Duration" ID="4370">
    <definition>&lt;def-root&gt;How long the grooming takes place.&lt;/def-root&gt;</definition>
    <semType name="Duration" ID="142"/>
  </FE>
  <FE bgColor="1E90FF" fgColor="FFFFFF" coreType="Peripheral" cBy="MJE" cDate="12/09/2005 03:52:14 PST Fri" abbrev="pur" name="Purpose" ID="9766">
    <definition>&lt;def-root&gt;The state-of-affairs that the &lt;fen&gt;Agent&lt;/fen&gt; is trying to bring about by grooming.&lt;/def-root&gt;</definition>
    <semType name="State_of_affairs" ID="177"/>
  </FE>
  <FE bgColor="006400" fgColor="FFFFFF" coreType="Peripheral" cBy="MJE" cDate="12/09/2005 03:54:00 PST Fri" abbrev="mea" name="Means" ID="9767">
    <definition>&lt;def-root&gt;The action performed by the &lt;fen&gt;Agent&lt;/fen&gt; to accomplish the grooming.&lt;/def-root&gt;</definition>
    <semType name="State_of_affairs" ID="177"/>
  </FE>
